my_str = 'Peter Piper Picked A Peck Of Pickled Peppers.'
sub_str = 'Peck'
new_str = 'Pack'
found = False
res = ''
i = 0
while i < len(my_str):
    if my_str[i:i+len(sub_str)] == sub_str and not found:
        res += new_str
        i += len(sub_str)
        found = True
    else:
        res += my_str[i]
        i += 1
